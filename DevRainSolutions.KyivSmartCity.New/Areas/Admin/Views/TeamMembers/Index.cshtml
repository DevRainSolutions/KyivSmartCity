@model IEnumerable<DevRainSolutions.KyivSmartCity.New.Models.TeamMember>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th></th>
        <th>
            Name
        </th>
        <th>
            ImageUrl
        </th>
        <th>
            Description
        </th>
        <th>
            Email
        </th>
        <th>
            Twitter
        </th>
        <th>
            Facebook
        </th>
        <th>
            LinkedIn
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
        <td>
			@item.Name
        </td>
        <td>
			@item.ImageUrl
        </td>
        <td>
			@item.Description
        </td>
        <td>
			@item.Email
        </td>
        <td>
			@item.Twitter
        </td>
        <td>
			@item.Facebook
        </td>
        <td>
			@item.LinkedIn
        </td>
    </tr>
}

</table>


